<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.niko.lab1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddContactActivity"
            android:label="AddContact" />
        <activity
            android:name=".ViewAllActivity"
            android:label="ViewAllContacts" />
        <activity
            android:name=".SearchActivity"
            android:label="SearchContacts" />
        <activity
            android:name=".AnimationActivity"
            android:label="@string/app_name1" />
        <activity
            android:name=".CanvasActivity"
            android:label="Canvas" />

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".CameraActivity"
            android:label="Camera" />

        <uses-feature
            android:name="android.hardware.camera"
            android:required="true" />

        <activity android:name=".EditActivity" android:label="Edit"></activity>
    </application>

</manifest>
